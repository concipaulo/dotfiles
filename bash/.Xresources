!#################################
Xft.dpi:                    96
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight
!#################################
! st config
*.alpha:0.85
blinktimeout: 800
! URxvt configs
Urxvt.title: URxvt
URxvt.scrollstyle: plain
/* URxvt.transparent: true */
/* URxvt.shading: 30 */
/* URxvt.depth: 32 */
/* urxvt*depth: 32 */
/* urxvt*background: rgba:0000/0000/0200/c800 */
URxvt.perl-ext-common: default,keyboard-select,url-select,resize-font,vim-scrollback
URxvt.jumpScroll: true
URxvt.loginShell: true
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.laucher: /usr/bin/qutebrowser
URxvt.url-select.undeline: true
Urxvt.keyboard-select.clipboard: true
URxvt.saveLines: 12000
URxvt*font: xft:M+ 1m:style=medium:pixelsize=14:antialias=true:autohint=true,PowerlineSymbols:style=Medium: size=8
!/* URxvt*fontBold: xft:M+ 1m:style=bold:pixelsize=14:antialias=true:autohint=true,PowerlineSymbols:style=Medium: size=8 */
!/* URxvt*font: xft:FantasqueSansMono Nerd Font Mono::style=Regular: size=11, PowerlineSymbols:style=Medium: size=8 */
URxvt*fontBold: xft:FantasqueSansMono Nerd Font Mono:style=Bold: size=11, PowerlineSymbols:style=Medium: size=8
URxvt*letterSpace: -1
URxvt*scrollBar: false
URxvt.iso14755: false
URxvt.cursorBlink: 1
URxvt.cursorUnderline: 0
URxvt.internalBorder: 5
URxvt.externalBorder: 0
!/* ; configure alt-s as the keybinding to enter vim scrollback mode. */
urxvt.vim-scrollback: M-f
!/* ; configure alt-p as the keybinding to paste mode. */
urxvt.vim-scrollback-paste: M-p
!/* ; configure the background and foreground colors used for the status bar while */
!/* ; in scrollback mode. */
urxvt.vim-scrollback-bg: 16
urxvt.vim-scrollback-fg: 10
!/* ; configure vim-scrollback specific matchers */
urxvt.vim-scrollback.pattern.1: \\B(/\\S+?):(\\d+)(?=:|$)
urxvt.vim-scrollback.launcher.1: vim +$2 $1
!#################################
! ROFI
!#################################
rofi.fullscreen: false
!rofi.separator-style: none
!rofi.hide-scrollbar: true
!rofi.show-icons: true
!rofi.drun-icon-theme: Papirus-Dark
!rofi.lines: 8
rofi.position:center
rofi.color-enabled: true
!vim filetype:Xresouces
