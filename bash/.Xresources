!#################################
Xft.dpi:                    96
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight
!#################################
! st configs
*.alpha:0.98
! URxvt configs
Urxvt.title: URxvt
URxvt.scrollstyle: plain
URxvt.depth: 32
URxvt.perl-ext-common: default,keyboard-select,url-select,resize-font
URxvt.jumpScroll: true
URxvt.loginShell: true
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.laucher: /usr/bin/qutebrowser
URxvr.url-select.undeline: true
Urxvt.keyboard-select.clipboard: true
URxvt.saveLines: 12000
URxvt*font: xft:SauceCodePro Nerd Font Mono:style=Light: size=11, PowerlineSymbols:style=Medium: size=9
URxvt*boldfont: xft:SauceCodePro Nerd Font Mono:style=Bold: size=11, PowerlineSymbols:style=Medium: size=9
URxvt*letterSpace: -1
! scrollbar - true (default) or false
URxvt*scrollBar: false
URxvt.iso14755: false
URxvt.cursorBlink: 1
URxvt.cursorUnderline: 0
URxvt.internalBorder: 0
URxvt.externalBorder: 0
!#################################
! ROFI Color theme
!#################################
rofi.fullscreen: false
rofi.separator-style: none
rofi.hide-scrollbar: true
rofi.show-icons: true
rofi.drun-icon-theme: Papirus-Dark
rofi.lines: 8
rofi.position:center
rofi.color-enabled: true
!vim filetype:Xresouces
